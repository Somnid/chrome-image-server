{
  "name": "Chrome Image Server",
  "version": "0.9",
  "description": "Description",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts" : [
        "js/libs/utilities.js",
        "js/libs/file-tools.js",
        "js/buffer-helper.js",
        "js/file-helper.js",
        "js/file-system.js",
        "js/http-helper.js",
        "js/http-parser.js",
        "js/image-router.js",
        "js/mime-mapper.js",
        "js/http-server.js",
        "js/chromep.js",
        "js/image-server.js",
        "js/server-background-controller.js",
        "js/background.js"
      ]
    }
  },
  "sockets" : {
	  "tcpServer" : {
		  "listen" : "*"
	   },
		"tcp" : {
	    "connect" : "*"
	  }
	},
  "permissions" : [
	  "<all_urls>",
	  "storage"
  ]
}
